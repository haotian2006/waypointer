{
  "name": "waypointer",
  "displayName": "Waypointer",
  "description": "yes",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/haotian2006/waypointer"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "waypointer.Save",
        "title": "Save At Current"
      },

      {
        "command": "waypointer.Back",
        "title": "Go To"
      }
    ],
    "keybindings": [
      {
        "command": "waypointer.Save",
        "key": "ctrl+alt+1"
      },
      {
        "command": "waypointer.BackWithoutClear",
        "key": "ctrl+alt+2"
      },
      {
        "command": "waypointer.Back",
        "key": "ctrl+alt+`"
      },
      {
        "command": "waypointer.Forward",
        "key": "ctrl+alt+3"
      },
      {
        "command": "waypointer.Clear",
        "key": "ctrl+alt+4"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
